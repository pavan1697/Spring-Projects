package com.pavan.springex;

public class Patient {
	private String name;
	private int age;
	Doctor doctor;
	Pharmacy pharmacy;
	public Patient(String name, int age, Doctor doctor, Pharmacy pharmacy) {
		super();
		this.name = name;
		this.age = age;
		this.doctor = doctor;
		this.pharmacy = pharmacy;
	}
	
}
